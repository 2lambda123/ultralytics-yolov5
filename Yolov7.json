# parameters
nc: 80  # number of classes

width_multiple: 0.50  # layer channel multiple

# anchors
anchors:
  - [10, 16,33,66,33,28,6]  # P3/8
  - [30,61, 62,45, 59,119]  # P4/16
  

# YOLOv5 backbone
backbone:
  
  [[-1, 1, [64, 3]],  # 0-P1/2
   [-1, 1, [128, 3, 2]],  # 1-P2/4
   [-1, 3, [128]],
   [-1, 1, [256, 3, 2]],  # 3-P3/8
   [-1, 9, [256]],
   [-1, 1, [512, 3, 2]],  # 5-P4/16
   [-1, 9, [512]],
   [-1, 1, [1024, 3, 2]],  # 7-P5/32
   [-1, 1, [1024, [5, 9, 13]]],
   [-1, 3, [1024, False]],  # 9
  ]

# YOLOv5 head
head:
  [[-1, 1, [512, 1, 1]],
   [-1, 1, [None, 2, 'nearest']],
   [[-1, 6], 1, [1]],  # cat backbone P4
   [-1, 3, C3, [512, False]],  # 13

   [-1, 1, [256, 1, 1]],
   [-1, 1, [None, 2, 'nearest']],
   [[-1, 4], 1,[1]],  # cat backbone P3
   [-1, 3, [256, False]],  # 17 (P3/8-small)

   [-1, 1, [256, 3, 2]],
   [[-1, 14], 1, [1]],  # cat head P4
   [-1, 3, C3, [512, True]],  # 20 (P4/16-medium)

   [-1, 1, [512, 3, 2]],
   [[-1, 10], 1, [1]],  # cat head P5
   [-1, 3, [1024, False]],  # 23 (P5/32-large)

   [[17, 20, 23], 1, [nc, anchors]],  # Detect(P3, P4, P5)
  ]

